{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "naver",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "settings": {},
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-empty-function": "off",
    "no-nested-ternary": "off",
    "no-useless-constructor": "off"
  }
}
